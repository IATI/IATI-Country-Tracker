# convincing npm to pick the latest version of a dependency is, uhm, lets go with too clever by half
# this script is a forced npm install of wanted dependencies

cd `dirname $0`

rm -rf node_modules
rm package-lock.json

npm install stupid-table-plugin
npm install async
npm install papaparse
npm install brace
npm install browserify
npm install chalk
npm install chartist
npm install co
