{
  "name": "ctrack",
  "description": "iati country tracker",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/IATI/IATI-Country-Tracker.git"
  },
  "dependencies": {},
  "devDependencies": {
    "watchify": "^0.6.1",
    "uglify-js": "^2.4.12",
    "browserify": "^3.30.2",
    "express": "^3.4.8",
    "wait.for": "^0.6.3",
    "yargs": "^1.1.3",
    "csv": "^0.3.7"
  },
  "scripts": {
    "build": "node_modules/browserify/bin/cmd.js -r ./js/ctrack.js:ctrack -d -o jslib/ctrack.js && node_modules/uglify-js/bin/uglifyjs jslib/ctrack.js -o jslib/ctrack.min.js",
    "watch": "node_modules/watchify/bin/cmd.js -r ./js/ctrack.js:ctrack -d -o jslib/ctrack.js -v",
    "bake": "node js/bake.js & npm run build",
    "serv": "npm run bake & npm run build & node js/serv.js"
  }
}
