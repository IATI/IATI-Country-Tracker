cd `dirname $0`
echo "pushing the static pages into the gh_pages branch"

git clone --no-checkout --branch gh-pages -- ../ static || echo "run rm -rf static if you want to reset the static output"

npm run build

# make real data

rm static/jslib
cp -r ../ctrack/jslib static/

rm static/art
cp -r ../ctrack/art static/

cd static
git add . --all
git commit -a -m "$*"
git push
cd ..

